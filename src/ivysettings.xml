<ivysettings>
	<settings defaultResolver="default" />
	<resolvers>
		<chain name="default">
			<ibiblio name="ibiblio" m2compatible="true" />
			<url name="csabagaspar" m2compatible="true">
				<artifact
					pattern="http://csabagaspar.github.io/maven-repository/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
			</url>
		</chain>
	</resolvers>
</ivysettings>
